{
  "require": {
    "ext-redis": "*",
    "illuminate/database": "^8.0",
    "pimple/pimple": "^3.2",
    "symfony/event-dispatcher": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Poet\\": "framework/"
    },
    "files": [
      "framework/helpers.php"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
